    "react-dom": "^18.3.1",
    "rimraf": "^4.4.1",
    "terser": "^5.31.6",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vitest": "^2.0.5"
